<%= form_for :item, :url => user_items_path, :html => { :multipart => true } do |f| %>
	<div class="name">
		<%= f.label :name %>
		<%= f.text_field :name %>
	</div>
	<div class="description">
		<%= f.label :description %>
		<%= f.text_area :desc %>
	</div>
	<p>
		<%= f.submit "Submit" %>
	</p>
<% end %>